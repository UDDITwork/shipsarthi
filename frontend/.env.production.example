# Production Environment Configuration

# API Configuration
REACT_APP_API_URL=https://api.shipsarthi.com/api

# WebSocket Configuration (Production)
REACT_APP_WS_URL=wss://api.shipsarthi.com
REACT_APP_WS_HOST=api.shipsarthi.com
REACT_APP_WS_PORT=443

# App Configuration
REACT_APP_NAME=Shipsarthi
REACT_APP_VERSION=1.0.0

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_CHAT=true

# External Services
REACT_APP_GOOGLE_MAPS_API_KEY=your-production-google-maps-api-key

# Environment
REACT_APP_ENV=production

# Security
REACT_APP_ENABLE_DEBUG=false
REACT_APP_LOG_LEVEL=error
